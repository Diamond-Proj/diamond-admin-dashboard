    "apisauce": "^3.0.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.40.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.368.0",
    "next": "15.5.3",
    "next-auth": "5.0.0-beta.13",
    "next-themes": "^0.3.0",
    "pg": "^8.14.1",
    "postgres": "^3.4.5",
